<div class="container-fluid">
    <div class="row">
        <div class="col-md-2">
        </div>
        
        <div class="col-md-8">
            <% if (!categorie.isEmpty()) { %>
                <div class="elements">
                    <% categorie.forEach(function(categoria) { %>
                        <div class="elemento">
                            <a href="/categoria/<%= categoria %>"><%= categoria %>
                                <div class="info-elemento nome">Nome:
                                    <% categoria %>
                                </div>
                            </a>
                        </div>
                    <% }); %>
                </div>
            <% } %>
            <% else { %>
                <h3>Non sono presenti categorie al momento</h3>
            <% } %>
        </div>

        <div class="col-md-2">
        </div>
    </div>
</div>